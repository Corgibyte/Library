@using Library.Models;

<h1>Patrons</h1>

@if (@Model.Count == 0)
{
  <h3>No Patrons have been added yet!</h3>
} 

<ul>
  @foreach(Patron patron in Model)
  {
    <li>@Html.ActionLink($"{patron.Name}", "Details", new { id = patron.PatronId })</li>
  }
</ul>

<p>@Html.ActionLink("Add new patron", "Create")</p>
<p>@Html.ActionLink("Home", "Index", "Home")</p>